	def __init__(self):
		self.menu_items={}
	def additems(self,item,cost):
		self.menu_items[item]=cost
	def show(self):
		for k,v in self.menu_items.items():
		    print(k,'=',v)
	def show_items(self,items):
		price=self.menu_items.get(items)
		print(price)
	def __add__(self,other1,other2):
		return menu(list(other1)+list(other2))
        

m=menu()
m+=('dosa',20)
m.show()
