class menu:
	def __init__(self):
		self.menu_items={}
	def additems(self,item,cost):
		self.menu_items[item]=cost
	def show(self):
		for k,v in self.menu_items.items():
		    print(k,'=',v)
	def show_items(self,items):
		price=self.menu_items.get(items)
		print(price)
	def __add__(self,other):
		self.menu_items[other[0]]=other[1]
		return self
